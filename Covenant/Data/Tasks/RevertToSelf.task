using System;

using SharpSploit.Credentials;

public static class Task
{
    public static string Execute()
    {
        using (Tokens t = new Tokens())
        {
            if (t.RevertToSelf())
            {
                return "Reverted to user: " + t.WhoAmI();
            }
            else
            {
                return "Failed to RevertToSelf.";
            }
        }
    }
}